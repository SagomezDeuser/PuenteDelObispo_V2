"main(mapping event)
{
  int incidencia, prioridad, reparacion;
  string nombre, usuario, lugar, elemento, descripcion_inci, causa_inci, descripcion_rep, fecha_hora, inst_bd, insert_sql, select_sql, titulo_em, texto_em;
  dyn_string fecha_hora_sep, confirmar_st;
  dyn_float confirmar;
  dyn_anytype resultado_consulta;

  if (txf_nombre.text != \"\" || txf_descripcion_incidencia.text != \"\")
  {
    titulo_em = \"Confirmar Incidencia\";
    texto_em = \"¿Está seguro de que quiere guardar la incidencia?\";
    ChildPanelOnCentralModalReturn(\"vision/emergentes/internas/confirmacion_personalizada.pnl\", \"Cambiar selección\",
                                   makeDynString(\"$TITULO:\" + titulo_em, \"$TEXTO:\" + texto_em), confirmar, confirmar_st); //Mostramos la ventana de confirmación para modificar o no

    if (dynlen(confirmar) != 0 && confirmar[1] == 1)
    {

      inst_bd = instancia_bd(); //Creamos la instancia para el insert posterior

      //Recuperamos los valores de los campos para insertarlos en la tabla
      fecha_hora = txf_fecha.text;
      fecha_hora_sep = strsplit(fecha_hora, \" \");
      strreplace(fecha_hora, \" \", \"T\");

      nombre = txf_nombre.text;
      usuario = txf_usuario.text;
      prioridad = cbb_prioridad.selectedPos;
      lugar = txf_lugar_incidencia.text;
      elemento = txf_elemento.text;
      descripcion_inci = txf_descripcion_incidencia.text;
      causa_inci = txf_causa_incidencia.text;

      if (cb_reparacion.state(0)) reparacion = 1;
      else reparacion = 0;

      descripcion_rep = txf_descripcion_reparacion.text;

      //Generamos la consulta y realizamos el insert a la BBDD

      insert_sql = \"INSERT INTO \" + inst_bd + \"[Incidencias] (Fecha_Hora, Fecha, Hora, Nombre_Incidencia, Usuario, Prioridad, \" +
                   \"Lugar, Elemento, Descripcion_Incidencia, Causa_Incidencia, Reparacion, Descripcion_Reparacion) \" +
                   \"VALUES('\" + fecha_hora + \"', '\" + fecha_hora_sep[1] + \"', '\" + fecha_hora_sep[2] + \"','\" + nombre + \"','\" + usuario + \"' , \" + prioridad +
                   \", '\" + lugar + \"', '\" + elemento + \"','\" + descripcion_inci + \"','\" + causa_inci + \"',\" + reparacion + \",'\" + descripcion_rep + \"')\";

      cmd_bd(\"SQL\", insert_sql);

      //Una vez que realicemos el insert, reseteamos los valores de los campos de texto y actualizamos la tabla

      txf_nombre.text = \"\";
      txf_fecha.text = getCurrentTime();
      txf_num.text = \"\";
      txf_usuario.text = getUserName();
      cbb_prioridad.selectedPos = 1;
      txf_lugar_incidencia.text = \"\";
      txf_elemento.text = \"\";
      txf_descripcion_incidencia.text = \"\";
      txf_causa_incidencia.text = \"\";
      cb_reparacion.state(0) = FALSE;
      txf_descripcion_reparacion.text = \"\";

      actualizar_tabla();
    }
  }
  else
  {
    txf_nombre.backCol(\"LIB_IRC_ROJO_01\");
    txf_descripcion_incidencia.backCol(\"LIB_IRC_ROJO_01\");
    delay(2, 000);
    txf_nombre.backCol(\"LIB_DEUSER_BLANCO\");
    txf_descripcion_incidencia.backCol(\"LIB_DEUSER_BLANCO\");
  }
}


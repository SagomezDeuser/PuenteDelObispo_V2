void actualizar_tabla()
{
  dyn_dyn_anytype resultado_consulta;
  string inst_bd, select_sql;
  inst_bd = instancia_bd();
  select_sql = \"SELECT [Nombre_Incidencia],[Prioridad],[Fecha_Hora], [Id] FROM \" + inst_bd + \"[Incidencias]\"  ;
  resultado_consulta = select_bd(\"SQL\", select_sql);
  tb_incidencias.deleteAllLines();

  for (int i = 1; i <= dynlen(resultado_consulta); i++)
  {
    tb_incidencias.appendLine(\"incidencia\", resultado_consulta[i][1], \"prioridad\", resultado_consulta[i][2], \"fecha\", resultado_consulta[i][3], \"id\", resultado_consulta[i][4]);
  }

  if (rb_orden.number == 0)
  {
    setValue(\"tb_incidencias\", \"sort\", FALSE, \"fecha\");
  }
  else if (rb_orden.number == 1)
  {
    setValue(\"tb_incidencias\", \"sort\", FALSE, \"prioridad\");
  }
}

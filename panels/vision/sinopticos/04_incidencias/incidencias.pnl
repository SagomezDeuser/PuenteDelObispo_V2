{
  string sentencia_sql, inst_bd, titulo_em, texto_em, select_sql;
  dyn_float confirmar;
  dyn_string confirmar_st;
  dyn_anytype resultado_consulta;

  if (tb_incidencias.selectLineN != -1)
  {
    titulo_em = \"Borrar Incidencia\";
    texto_em = \"¿Está seguro de que quiere borrar la incidencia seleccionada?\";
    ChildPanelOnCentralModalReturn(\"vision/emergentes/internas/confirmacion_personalizada.pnl\", \"Cambiar selección\",
                                   makeDynString(\"$TITULO:\" + titulo_em, \"$TEXTO:\" + texto_em), confirmar, confirmar_st); //Mostramos la ventana de confirmación para modificar o no

    if (dynlen(confirmar) != 0 && confirmar[1] == 1)
    {
      inst_bd = instancia_bd();

      sentencia_sql = \"DELETE FROM \" + inst_bd + \"[Incidencias] WHERE Id =\" + txf_num.text;

      cmd_bd(\"SQL\", sentencia_sql);

//Una vez que realicemos el delete, reseteamos los valores de los campos de texto y actualizamos la tabla

      txf_nombre.text = \"\";
      txf_fecha.text = getCurrentTime();
      txf_num.text = \"\";
      txf_usuario.text = getUserName();
      cbb_prioridad.selectedPos = 1;

  dyn_string resultStrings;
  bool permiso = permisos_usuario_operacion(12, FALSE);

  if (permiso && row != -1)
  {
    string question = getCatStr(\"mensajes_emergentes\", \"borrrar_usuario\");
    dyn_float  resultFloats;

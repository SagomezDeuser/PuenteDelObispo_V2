permisos_visible(string grupo)
{
  int            j, k;
  bit32          b32;
  string         sBit, sCol;
  dyn_langString desliste;

  m_um = UserManagement::getInstance();

  bits_permisos_vivibles(true);
  tb_permisos_10_16.deleteAllLines();

  m_group = nullptr;
  m_group = m_um.getGroupByName(grupo);

  if (m_group == nullptr)
  {
    errClass error = makeError(0, PRIO_INFO, ERR_PARAM, 0, \"\", 0, 0, __FILE__ + \": m_group not set\");
    throwError(error);
    return;
  }

  b32 = m_group.getPermissionSet();
  desliste = m_um.getAllPermissionDescriptions();

  tb_permisos_10_16.appendLine(\"#1\", \"\");

  for (j = 0; j < 32; j++)
  {
    k = j + 2; sCol = \"#\" + k;
    sBit = (getBit(b32, j)) ? 1 : 0;
    setValue(\"tb_permisos_10_16\", \"cellValueRC\", 0, sCol, sBit);

    if (dynlen(desliste) > j) setValue(\"tb_permisos_10_16\", \"columnToolTipText\", k - 1, desliste[j + 1] + \"(\" + (j + 1) + \")\");
  }
}


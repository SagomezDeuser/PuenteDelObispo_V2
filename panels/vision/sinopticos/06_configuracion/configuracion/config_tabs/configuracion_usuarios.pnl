"void main()
{
  int groupId = -1;
  dyn_float df;
  dyn_string ds;

  OaAuthFactory factory;
  mixed auth = factory.getAuth(); // Get the current authentication method
  bool permiso = permisos_usuario_operacion(12,FALSE);

  if (permiso)
  {

    if (auth.getAuthType() != OaAuthenticationType::Standard) // AD or \"User defined\"
    {
      ChildPanelOnCentralModalReturn(
        \"vision/ud_groupSelect.pnl\",
        getCatStr(\"userMan\", \"groupSelect\"),
        makeDynString(),
        df,
        ds);
    }
    else
    {
      ChildPanelOnCentralModalReturn(\"vision/emergentes/internas/nuevo_grupo.pnl\",
                                     getCatStr(\"userMan\", \"addinggroup\"),
                                     makeDynString(), df, ds);

      if (dynlen(df) < 1 || df[1] == -1)
      {
        return;
      }

      if (ds[1] == \"\" || strpos(ds[1], \" \") >= 0)
      {
        ChildPanelOnCentralModalReturn(\"vision/emergentes/internas/mensaje_error_grupos.pnl\",
                                       getCatStr(\"para\", \"warning\"),
                                       makeDynString(getCatStr(\"mensajes_emergentes\", \"grupo_no_valido\")),
                                       df, ds);
        return;
      }

      string errStr;

      if (!UserManagementHooks::checkGroupName(ds[1], errStr))
      {
        ChildPanelOnCentralModalReturn(\"vision/emergentes/internas/mensaje_error_grupos.pnl\",
                                       getCatStr(\"para\", \"warning\"),
                                       makeDynString(errStr),
                                       df, ds);
        return;
      }

      // ds[1]: groupName
      // ds[2]: txf_nombre_completo
      // ds[3]: txf_descripcion
      shared_ptr<UserManagementGroup> group = new UserManagementGroup(ds[1]);
      dyn_string errorList;

      if (group.getError() != -1)
      {
        group.setFullName(ds[2]);
        group.setComment(ds[3]);
        m_um.addGroup(group);

        if (m_um.saveChangesError(errorList))
        {
          groupId = group.getId();
        }
      }

      if (groupId < 0)
      {
        ChildPanelOnCentralModalReturn(
          \"vision/emergentes/internas/mensaje_error_grupos.pnl\",
          getCatStr(\"para\", \"warning\"),
          makeDynString(getCatStr(\"mensajes_emergentes\", \"grupo_no_valido\")),
          df,
          ds);
        return;
      }

      m_group = nullptr;
      m_group = m_um.getGroup(groupId);

      if (m_group != nullptr)
      {
        datos_grupo_seleccionado(m_group.getName());
      }
    }
  }
}

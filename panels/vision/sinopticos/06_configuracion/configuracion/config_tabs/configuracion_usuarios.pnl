"main(mapping event)
{
  string user, estado;
  shape table;
  bool enable;
  dyn_string confirmar_st;
  dyn_float confirmar;

  bool permiso = permisos_usuario_operacion(12, FALSE);

  if (permiso)
  {
    table = getShape(this.panel, \"users_table_gestion\");
    getValue(table, \"cellValueRC\", table.selectLineN(), \"Usuario\", user);
    getValue(table, \"cellValueRC\", table.selectLineN(), \"Estado\", estado);

    if (table.selectLineN != -1)
    {
      if (estado == \"HABILITADO\" || estado == \"ENABLED\")
      {
        enable = setUserEnabledPVSS(getUserId(user), FALSE);
      }
      else
      {

        ChildPanelOnCentralModalReturn(\"vision/emergentes/internas/cambiar_password.pnl\",
                                 \"Cambiar_Password\",
                                 makeDynString(\"$username:\" + user, \"$sin_verificar:\" + true), confirmar, confirmar_st);

        if (dynlen(confirmar) != 0 && confirmar[1] == 1) enable = setUserEnabledPVSS(getUserId(user), TRUE);
      }
    }
    else
    {
      mostrar_msg_emergente(4, \"usuario_no_seleccionado\");
    }
  }

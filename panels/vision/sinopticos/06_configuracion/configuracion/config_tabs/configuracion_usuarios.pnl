void rellenar_tabla_usuarios(string dp, dyn_string users, string dp2, dyn_bool mode_aux)
{
  dyn_string mode;
  dyn_bool mode_aux;
  dpGet(\"_Users.Enabled\", mode_aux);
  users_table_gestion.deleteAllLines;

  for (int i = 1; i <= dynlen(mode_aux); i++)
  {
    if (mode_aux[i])
    {


      if (getLangIdx(getActiveLangId()) == 0)
      {
        mode[i] = \"HABILITADO\";
      }

      else if (getLangIdx(getActiveLangId()) == 1)
      {
        mode[i] = \"ENABLED\";
      }
    }
    else
    {
      if (getLangIdx(getActiveLangId()) == 0)
      {
        mode[i] = \"DESHABILITADO\";
      }

      else if (getLangIdx(getActiveLangId()) == 1)
      {
        mode[i] = \"DISABLED\";
      }
    }

  }

  for (int i = 1; i <= dynlen(users); i++)
  {
    if (users[i] != \"para\" && users[i] != \"root\" && users[i] != \"admin\")
    {
      setValue(\"users_table_gestion\", \"appendLine\", \"Usuario\", users[i], \"Estado\", mode[i]);
    }
  }
}

dyn_string obtener_nombre_usuarios()
{
  dyn_string usernames;

  for (int i = 1; i <= dynlen(m_users); i++)
  {
    string name = m_users[i].getName();
    dynAppend(usernames, name);
  }

  return usernames;
}

void guardar_propiedades_grupo(string msgKey = \"cannotmodify\")
{
  dyn_string errorList;

  if (!m_um.saveChangesError(errorList))
  {
    ChildPanelOnCentralModal(
      \"vision/MessageWarning\",
      getCatStr(\"para\", \"warning\"),
      makeDynString(getCatStr(\"userMan\", msgKey) + \"\\n\" + strjoin(errorList, \"\\n\")));
  }
  else
  {
    if (m_group != nullptr)
    {
      datos_grupo_seleccionado(m_group.getName());
    }
    else if (dynlen(grupos_table_gestion.lineCount()) > 0)
    {
      grupos_table_gestion.selectLineN(0);
    }
  }
}

void datos_grupo_seleccionado(string groupName)
{
  if (groupName == \"\")
    return;

  bool           bWinGroup = false;
  OaAuthFactory  factory;
  mixed          authMethod = factory.getAuth();
  bool           editEnabled = (authMethod.getAuthType() == OaAuthenticationType::Standard);
  dyn_string     cbb_miembros;

  m_group = nullptr;
  m_group = m_um.getGroupByName(groupName);

  if (m_group == nullptr)
  {
    return;
  }

  m_users = m_um.getSavedUsersByGroupId(m_group.getId());

  cbb_miembros = obtener_nombre_usuarios();

  setMultiValue(\"cbb_miembros\",    \"items\",   cbb_miembros,
                \"cbb_miembros\",    \"text\",    dynlen(cbb_miembros) > 0 ? cbb_miembros[1] : \"\",
                \"txf_nombre_grupo\",        \"text\",    m_group.getName(),
                \"txf_nombre_completo\",   \"text\",    m_group.getFullName(),
                \"txf_descripcion\",    \"text\",    m_group.getComment());
}

void cmdClose()
{
  dyn_float df, dfDummy;
  dyn_string ds, dsDummy;

  if (m_group == nullptr || m_group.getId() == DEFAULT_USERID)
  {
    PanelOffReturn(dfDummy, dsDummy);
    return;
  }

  if (m_group.isDirty())
  {
    ChildPanelOnCentralModalReturn(\"vision/MessageInfo\",
                                   getCatStr(\"para\", \"information\"),
                                   makeDynString(getCatStr(\"userMan\", \"changed_paneloff\"),
                                       getCatStr(\"para\", \"yes\"),
                                       getCatStr(\"para\", \"no\")),
                                   df, ds);

    if (dynlen(df) < 1 || df[1] < 1)
    {
      return;
    }
  }

  PanelOffReturn(dfDummy, dsDummy);
}


void rellenar_tabla_grupos(string dp, dyn_string groupNames)
{
  int n_grupos = dynlen(groupNames);
  bool permiso = permisos_usuario_operacion(20,FALSE,FALSE);
  grupos_table_gestion.deleteAllLines;

  for (int i = 1; i <= n_grupos; i++)
  {
    if (i == 1 || i == 2 || i == 3)
    {
      if (permiso)grupos_table_gestion.appendLine(\"grupos\", groupNames[i]);
    }
    else
    {
      grupos_table_gestion.appendLine(\"grupos\", groupNames[i]);
    }
  }
}

void closePanel()
{
  PanelOffReturn(makeDynFloat(0), makeDynString());
}


